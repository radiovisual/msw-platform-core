{
  "name": "msw-platform-core",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run typecheck && jest",
    "build": "tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,less}\"",
    "lint:staged": "npm run lint && npm run format",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@commitlint/config-conventional": "^19.8.1",
    "@storybook/addon-essentials": "8",
    "@storybook/addon-interactions": "8",
    "@storybook/addon-toolbars": "8",
    "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
    "@storybook/preset-create-react-app": "8",
    "@storybook/react": "8",
    "@storybook/react-webpack5": "8",
    "@storybook/test": "^8.3.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.4",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^19.1.8",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-eslint": "^10.1.0",
    "commitlint": "^19.8.1",
    "css-loader": "^7.1.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.4",
    "lint-staged": "^16.1.2",
    "msw": "^2.10.3",
    "msw-storybook-addon": "^2.0.5",
    "node-fetch": "^2.7.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^2.8.7",
    "storybook": "8",
    "ts-jest": "^29.1.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "react": "^19.1.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint",
    "*.{js,jsx,ts,tsx,json,less}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "msw": {
    "workerDirectory": [
      ".storybook\\public"
    ]
  }
}